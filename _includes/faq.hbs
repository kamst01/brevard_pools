{{#with faq}}
<section class="bg-white" id="faq" aria-labelledby="faqHeading">
  <div class="px-4 py-16 mx-auto divide-y-2 divide-gray-200 max-w-7xl sm:py-24 sm:px-6 lg:px-8">
    <h2 id="faqHeading" class="text-3xl font-extrabold text-gray-900" aria-hidden="true">
      {{heading}}
    </h2>
    <div class="pt-10 mt-6">
      <dl class="space-y-10 md:space-y-0 md:grid md:grid-cols-2 md:grid-rows-2 md:gap-x-8 md:gap-y-12" aria-labelledby="listOfFAQHeading">
        <h2 id="listOfFAQHeading" class="sr-only" aria-hidden="true">List of Frequently Asked Questions</h2>
          {{#each faqs}}
            <div>
              <dt class="text-lg font-semibold leading-6 text-gray-900">
                {{ question }}
              </dt>
              <dd class="mt-2 text-base text-gray-600">
                {{ answer }}
              </dd>
            </div>
          {{/each}}
      </dl>
    </div>
  </div>
</section>
{{/with}}